loop:
    jmpi @loop

print-char:
    # get args
    addi r0, rsp, 12
    addi r1, rsp, 8
    addi r2, rsp, 4
    mov r0, [r0] # char start address
    mov r1, [r1] # x offset
    mov r2, [r2] # y offset
    prn r1
    prn r2

    # setup char ptr
    mov r3, r0

    # setup video mem ptr
    li r4, 0x10000
    # add r1 (x offset)
    add r4, r4, r1
    # add r2 (y offset) * 160
    mov r5, r2
    muli r5, r5, 160
    add r4, r4, r5

    # write line
    sb r4, [r3]
    addi r3, r3, 1
    addi r4, r4, 1
    sb r4, [r3]
    addi r3, r3, 1
    addi r4, r4, 1
    sb r4, [r3]
    addi r3, r3, 1
    addi r4, r4, 1
    sb r4, [r3]
    addi r3, r3, 1
    addi r4, r4, 157

    # write line
    sb r4, [r3]
    addi r3, r3, 1
    addi r4, r4, 1
    sb r4, [r3]
    addi r3, r3, 1
    addi r4, r4, 1
    sb r4, [r3]
    addi r3, r3, 1
    addi r4, r4, 1
    sb r4, [r3]
    addi r3, r3, 1
    addi r4, r4, 157

    # write line
    sb r4, [r3]
    addi r3, r3, 1
    addi r4, r4, 1
    sb r4, [r3]
    addi r3, r3, 1
    addi r4, r4, 1
    sb r4, [r3]
    addi r3, r3, 1
    addi r4, r4, 1
    sb r4, [r3]
    addi r3, r3, 1
    addi r4, r4, 157

    # write line
    sb r4, [r3]
    addi r3, r3, 1
    addi r4, r4, 1
    sb r4, [r3]
    addi r3, r3, 1
    addi r4, r4, 1
    sb r4, [r3]
    addi r3, r3, 1
    addi r4, r4, 1
    sb r4, [r3]
    addi r3, r3, 1
    addi r4, r4, 157

    # write line
    sb r4, [r3]
    addi r3, r3, 1
    addi r4, r4, 1
    sb r4, [r3]
    addi r3, r3, 1
    addi r4, r4, 1
    sb r4, [r3]
    addi r3, r3, 1
    addi r4, r4, 1
    sb r4, [r3]
    addi r3, r3, 1
    addi r4, r4, 157

    ret

